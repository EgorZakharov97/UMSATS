<body>
  
  <% include partials/navbar %>

  <div>
    <div class="w-container">
      <div class="w-form">
        <form id="email-form" action="/items/<%= item._id %>?_method=PUT" method="POST">
          <label for="item[name]">Edit Name</label>
          <input required type="text" class="w-input" maxlength="256" name="name" data-name="Name" id="name" value="<%=item.name%>">
          <label for="email">Edit Category</label>
          <select id="category" name="item[category]" class="w-select">
            <option <% if(item.category == "Category1"){ %> selected <% } %> value="Category1">Category1</option>
            <option <% if(item.category == "Category2"){ %> selected <% } %> value="Category2">Category2</option>
            <option <% if(item.category == "Category3"){ %> selected <% } %> value="Category3">Category3</option>
            <option <% if(item.category == "Category4"){ %> selected <% } %> value="Category4">Category4</option>
          </select>
          <% if(item.disposable){ %>
              <label for="imageIn">Update quantity</label>
              <input type="number" name="item[quantityAvailable]" class="form-control" id="imageIn" value="<%= item.quantityAvailable %>">
          <% } %>
          <label for="location">Edit Location</label>
          <input type="text" class="w-input" maxlength="256" name="item[location]" data-name="location" id="location" required="" value="<%=item.location%>">
          <label for="Description">Edit Description</label>
          <textarea required data-name="Description" maxlength="5000" id="Description" name="item[description]" class="w-input"><%= item.description %></textarea>
          <input type="submit" value="Submit" data-wait="Please wait..." class="w-button">
        </form>
      </div>
      <div>
        <form action="/items/<%= item._id %>?_method=DELETE" method="POST">
          <button class="btn-delete">Delete this item</button>
        </form>
      </div>
    </div>
  </div>
