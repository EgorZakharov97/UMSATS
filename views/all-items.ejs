                <div class="column-7 w-col w-col-6">
                  <h4 class="item-header"><%= item.name %></h4>
                  <div class="item-category"><%= item.category %></div>
                </div>
              </div>
              <p class="paragraph-2"><%= item.description %></p>
              <a href="/items/<%= item._id %>" class="link-block w-inline-block">
